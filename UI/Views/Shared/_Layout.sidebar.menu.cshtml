@{
    var rootMenus = this.ViewData["RootMenus"] != null ? this.ViewData["RootMenus"] as List<Model.UserMenuModel> : new List<Model.UserMenuModel>();
    var otherMenus = this.ViewData["OtherMenus"] != null ? this.ViewData["OtherMenus"] as List<Model.UserMenuModel> : new List<Model.UserMenuModel>();
}
<ul class="nav nav-list">
    @foreach (var item in rootMenus)
    {
        var childMenus = otherMenus.Where(x => x.ParentCode.StartsWith(item.MenuCode)).OrderBy(x => x.Sort).ToList();
        if (childMenus.Count == 0)
        {
            <li class="">
                <a href="@item.LinkAddress" data-code="@item.MenuCode">
                    <i class="@item.IconScript"></i>
                    <span class="menu-text"> @item.MenuName </span>
                </a>

                <b class="arrow"></b>
            </li>
        }
        else
        {
            <li class="">
                <a href="javascript:;" class="dropdown-toggle" data-code="@item.MenuCode">
                    <i class="@item.IconScript"></i>
                    <span class="menu-text">
                        @item.MenuName
                    </span>

                    <b class="arrow fa fa-angle-down"></b>
                </a>
                <b class="arrow"></b>
                <ul class="submenu">
                    @foreach (var child in childMenus)
                    {
                        <li class="">
                            <a href="@child.LinkAddress" data-code="@child.MenuCode">
                                <i class="@child.IconScript"></i>
                                @child.MenuName
                            </a>

                            <b class="arrow"></b>
                        </li>
                    }
                </ul>
            </li>

        }
    }
</ul>

